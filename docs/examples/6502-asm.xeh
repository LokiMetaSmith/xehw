
: unknown-word-handler
    1 str-split
    case 
        "#" of
            1 str-split
            "$" equal? if HEX str>number else drop nil endif
        endof
        "$" of
            str>number
        endof
        "@" of
            . "LABEL"
        endof
            nil
    endcase
;

0 var current-org
nil var current-op

: check-opcode

;

: set-opcode
    current-op if check-opcode endif
    current-op !
;

: ADC ( [ 0x69 . "I" ] ) set-opcode ;
    
ADC
